		}
	}
}
function uf_add() {
	obj=pubDataSet[0];
	var o= obj.childNodes(0).childNodes(0).childNodes(0);
	stmp="<option value='页签'><font size=2>页签</font></option>";
	ss=SKDBListBox1.outerHTML;
	SKDBListBox1.outerHTML=ss.substring(0,(ss.length-9))+stmp+"</select>";
	var s1=obj.childNodes(0).outerHTML;
	obj.childNodes(0).outerHTML=s1.substring(0,s1.length-21)+"<td style='background-color:white;border-left:1px solid #8BA7B6;border-top:1px solid #8BA7B6;border-right:1px solid #8BA7B6;' width=80px height=20px align=center>"+stmp+"</td>"
