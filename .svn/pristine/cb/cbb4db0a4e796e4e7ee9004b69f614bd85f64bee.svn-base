		var svalue= o.childNodes(i).innerText;
		if(stmp==""){
			stmp="<option value='"+svalue+"'>"+svalue+"</option>";
		}else{
			stmp=stmp+"<option value='"+svalue+"'>"+svalue+"</option>";
		}
	}
	ss=SKDBListBox1.outerHTML;
	SKDBListBox1.outerHTML=ss.substring(0,(ss.length-9))+stmp+"</select>";
	SKDBedit1.onchange = function uf_id_onchange() {
								var sR = CheckContSameName(pubDataSet[3],SKDBedit1.value,obj,pubDataSet[2]) ;
								if(sR != ""){
									alert(sR);
									event.returnValue = false ;
								}
							}
	SKDBcombobox3.value = obj.style.position ;
	if(obj.style.position == "static") {
		txtLeft.disabled = true ;
		txtTop.disabled = true ;	
	}else{
		txtLeft.disabled = false ;
		txtTop.diabled = false ;
	}
}
function uf_onchange(){
	var ilen=SKDBListBox1.options.length;
	for(var i=0;i<ilen;i++){
		if(SKDBListBox1.options(i).selected){
			SKDBedit2.value=SKDBListBox1.options(i).value;
			break;
		}
	}
}
function uf_ment() {
	obj=pubDataSet[0];
	for (var i=0 ; i<SKDBListBox1.length; i++){
		if(SKDBListBox1.options(i).selected){
			var d=SKDBListBox1.options(i).value ;
			SKDBListBox1.options(i).text=SKDBedit2.value ;
			SKDBListBox1.options(i).value=SKDBedit2.value ;
			obj.value=SKDBListBox1.value;
			if(SKDBedit2.value.length>7){
				alert("请缩减您的标题,最多只能七个字")
				return
			}
			var o= obj.childNodes(0).childNodes(0).childNodes(0);
			o.childNodes(i).innerHTML="<font size=2>"+SKDBedit2.value+"</font>";
