}
function uf_del() {
	var k=SKDBListBox1.selectedIndex;
	if(SKDBListBox1.options.length<=1){
		alert("页签不能少于一个")
		return
	}else{
		SKDBListBox1.options.remove(k);
	}
	obj=pubDataSet[0];
	var o= obj.childNodes(0).childNodes(0).childNodes(0);
	o.deleteCell(k);
	obj.childNodes(k+1).outerHTML="";
}

function uf_ok(){
	obj.style.position = SKDBcombobox3.value ;
			
			
	window.close();
}
function uf_changeVal(){
	if(SKDBcombobox3.value=="static"){
		txtLeft.value="";
		txtTop.value="";
