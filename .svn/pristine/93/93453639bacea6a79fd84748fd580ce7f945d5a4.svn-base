<script>function uf_open() {
//	var sHtml = GetAllDjFileName("/fceformext/res/") ;
//	cboUrl.outerHTML = SelectAddOption(cboUrl,sHtml);
	
	var obj=pubDataSet[0] ;
	SetTextValue(obj.srcsk,txtUrl)
	if(IsSpace(obj.cover)){
		cboCover.value = "9" ;
	}else{
		cboCover.value = obj.cover ;
	}
	PropWinOnload();
}
function uf_ok() {
	if(PropWinClickOk())return;
	var obj=pubDataSet[0] ;
	var s1 = txtUrl.value ;
	//if (cboUrl.selectedIndex > -1 )s1 = cboUrl.options(cboUrl.selectedIndex).text ;
	var s2 = encodeURI(s1);
	if(s1 != s2){
		alert("路径中不能包含中文及特殊字符！");
		return;
	}else{
		obj.src= s1 ;
		obj.srcsk=s1 ;
		obj.cover = cboCover.value ;
		window.close();
	}
}
