/* CSS Document */
body,td,th {
	font-size: 12px;
	margin: 0px;
	padding: 0px;
	
}
/* 表单 */
.kuan1 {
	margin: 2px;
	border: 1px solid #6d96c7;
	padding: 3px;
}
.kuans2 {
	background-color: #FFFFFF;
	border: 1px solid #BCBCBC;
	width: 30px;
}
/* 按钮 */
.button1 {
	font-family: "宋体";
	font-size: 12px;
	color: #435374;
	text-decoration: none;
	background-image: url(../images/tab_02.jpg);
	background-repeat: repeat-x;
	background-position: top;
	text-align: center;
	width: 20px;
	border: 1px solid #6d96c7;
	}

.button2 {
	font-family: "宋体";
	font-size: 12px;
	color: #435374;
	text-decoration: none;
	background-image: url(../images/tab_02.jpg);
	background-repeat: repeat-x;
	background-position: top;
	text-align: center;
	border: 1px solid #6d96c7;
	padding-top: 2px;
	 cursor: hand
}
.buttons3 {
	height: 20px;
	border: 1px solid #82b0e7;
	font-family: "宋体";
	font-size: 12px;
	color: #486086;
	text-decoration: none;
	background-image: url(../images/tab_3.jpg);
	background-repeat: repeat-x;
	background-position: bottom;
	text-align: center;
	cursor: hand;
	padding-top: 2px;
	margin-right: 3px;
	margin-top: 3px;
	margin-bottom: 3px;
}
/* 链接颜色 */
a:link {color: #FF6600; text-decoration:none;}
a:visited {color: #FF6600;text-decoration:none;}
a:hover {color: #FF6600;POSITION: relative; TOP: 1px;}
a:active {color: #486086;}

/* 表格单颜色*/
.table01 {
	border: 1px solid #8aa9ca;
	color: #435374;
	background-image: url(../images/tab_01.jpg);
	height: 26px;
	margin-top: 5px;
	font-size: 12px;
	background-repeat: repeat-x;
	background-position: top;
	padding-left: 5px;
}
.table02{
	background-color: #f2f5fa;
	margin-bottom: 8px;
	padding-top: 5px;
	padding-right: 5px;
	padding-bottom: 8px;
	padding-left: 5px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-right-color: #8aa9ca;
	border-bottom-color: #8aa9ca;
	border-left-color: #8aa9ca;
	color: #435374;
}
.table02 td {
	line-height: 22px;
	padding-right: 2px;
	padding-left: 2px;
	padding-top: 2px;
	padding-bottom: 2px;
}
.Atable{
	border-collapse:collapse;
	background:#ffffff;
	padding:3px;
	color: #486086;
	margin-top: 5px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	border: 1px solid #b8cce4;
	line-height: 22px;
}
.Atable td{
	border:1px solid #B7D3EA;
	color:#486086;
	padding: 3px;
}
.Atable .c1{
	font-weight: normal;
	color:#435374;
	background-image: url(../images/tab_04.jpg);
	font-size: 12px;
	background-repeat: repeat-x;
	line-height: 20px;
	background-position: top;
}
.Atable .c1_1{
	font-weight: bolder;
	color:#FFFFFF;
	font-size: 12px;
	line-height: 20px;
}
.Atable .c2{
	line-height: 30px;
	color: #18346f;
}
.Atable .c2 a:link {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.Atable .c2 a:visited {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.Atable .c2 a:hover {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.Atable .c3{
	line-height: 30px;
	color: #18346f;
}
.Atable .c3 a:link {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.Atable .c3 a:visited {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.Atable .c3 a:hover {
	font-size: 12px;
	color: #18346f;
	text-decoration: none;
}
.text {
	font-size: 12px;
	font-weight: bold;
	color: #18346f;
	vertical-align: middle;
}
