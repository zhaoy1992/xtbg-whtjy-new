public class MeetingNoteTemplateBean extends XtDbBaseBean{	
	
	public MeetingNoteTemplateBean() {
		super("OA_MEETING_NOTE_TEMPLATE", "m_id");
	}
	private String m_id;		//主键id
	private String m_name;		//短信模板名称
	private String m_notice_id;		//会议id
	private String m_contents;		// 短信内容
	/**
	*<b>Summary: 获取主键id</b>
	*/
