body {margin: 0;padding: 0;background: #fff;}
#kjff_wrap{width: 995px;height: 600px;margin: 0 auto; background: #fff}
#kjff_top{width: 995px;height: 340px;margin-top: 50px;}
#kjff_bottom{width: 995px;height: 250px;}
#kjff_left{width:490px;height:330px;float:left;}
#kjff_right{width:490px;height:330px;float:left;margin-left: 10px;border:1px solid #417EB7;} /*望城科技反腐首页篮板样式，此样式已放到公用style.css里去了*/
/*#kjff_right{width:490px;height:330px;float:left;margin-left: 10px;border:1px solid #CE2C1F;}*/ /*望城科技反腐首页红版,此样式已放到公用style.css里去了*/

/*左边模块样式*/
.kjff_left_content{width: 490px;height: 165px;border:1px solid #417EB7;border-top: none;} /*望城科技反腐首页篮板,此样式已放到公用style.css里去了*/
/*.kjff_left_content{width: 490px;height: 165px;border:1px solid #CE2C1F;border-top: none;}*/ /*望城科技反腐首页红版,此样式已放到公用style.css里去了*/
.kjff_left_content ul{width:490px;height:auto;margin-top:5px; padding-left:0px; display:block;list-style: none;font-size: 12px;}
.kjff_left_content ul li{width: 480px;height: 25px;padding:0 4px; border-bottom: 1px dotted #ccc;line-height: 25px;overflow: hidden;}
.kjff_left_content ul li:hover{background: #EEFFEE;cursor: pointer;}
.kjff_left_content ul li span{display: block;float: left;}
.kjff_left_content .kjff_left_content_first{width: 415px;overflow: hidden;}
.kjff_left_content .kjff_left_content_second{width: 65px;overflow: hidden;}

.kjff_title_wrap{width: 100%;height: 30px;line-height: 30px;background: #2C95E5;} /*望城科技反腐首页篮板,此样式已放到公用style.css里去了*/
/*.kjff_title_wrap{width: 100%;height: 30px;line-height: 30px;background: #CE2C1F;}*/ /*望城科技反腐首页红版,此样式已放到公用style.css里去了*/
.kjff_title{width: auto;height: 30px;display: block;float: left; margin-left: 4px;color: #fff;font-weight: bold;font-size: 14px;}
.kjff_title_more{width: 65px;height: 30px;display: block;float: right;font-size: 12px;}
.kjff_title_more a {color: #fff;font-size: 12px;text-decoration: none;}
.kjff_title_more a:hover{text-decoration: underline;}

/*右边模块样式*/
#kjff_right_content{width: 480px;height: auto;margin-left: 10px;}
.kjff_right_title{color: #2C95E5;font-weight: bold;font-size: 14px;line-height: 30px;}
.kjff_rigt_content_show{width:140px;height:31px;margin-left: 15px;margin-bottom:12px; padding:40px 0 0 80px; float: left;}
.kjff_rigt_content_show a{color: #fff;font-size: 14px;font-weight: bold;text-decoration: none;}
.kjff_rigt_content_show a:hover{color: red;text-decoration: underline;}
#img1{background: url("../images/btn01.gif") no-repeat;}
#img2{background: url("../images/btn02.gif") no-repeat;}
#img3{background: url("../images/btn03.gif") no-repeat;}
#img4{background: url("../images/btn04.gif") no-repeat;}

#kjff_rigt_count{width: 370px;height: 45px;margin-top:20px;padding-left:120px;font-size:14px; line-height:45px;float:left;background: url("../images/dytjjg.gif") no-repeat;}
