.subnav_more_list{ border:solid 1px #c4c4c4; background:#FFF; width:115px; float:left; position:absolute; z-index:999; right:561px; top:34px; clear:both;}
.subnav_more_list ul li{ padding-left:10px; float:left; line-height:30px; height:30px; overflow:hidden; background:none;}

/* 纵向二级菜单 */
.main_content{width:100%; margin:0px auto; }
.content{ width:100%; margin:0px auto; margin-top:20px;}
.lay_1{ float:left; width:180px; margin-right:20px; display:inline;}
.lay_1 h3 {float:left; height:31px; width:180px; margin-bottom:10px; }
.lay_1 h3 b{ width:116px; height:31px; line-height:31px; text-align:center; color:#FFF; font-size:14px; background:url(../images/box_bj.png) no-repeat 0px 0px; margin-left:30px; display:inline; float:left;  }
.lay_1 h3 b a{ color:#FFF;}
.lay_1 ul li{float:left; height:30px; line-height:30px; overflow:hidden; width:150px; font-size:14px; padding-left:30px;}
.lay_1 ul li a{ color:#000;}.content .lay_1 ul li a:hover { color:#3c86db;}
.lay_1 .lay_a_xuan{color:#3c86db; font-weight:bold; background:url(../images/pic.png) no-repeat -97px -170px; width:150px; height:30px; float:left}
.lay_1 .lay_a_xuan a{color:#3c86db; }

.lay_2{ overflow:hidden; zoom:1;margin-top:20px;}

/* 页角 */
#footer{ width:100%; background:#333438; height:120px; text-align:center; line-height:30px; color:#c0c0c0; padding-top:25px; font-family:"microsoft yahei"; float:left; margin-top:20px;}
#footer a{ color:#c0c0c0;}
#footer a:visited{ color:#c0c0c0;}
#footer a:hover{ color:#FFF;}