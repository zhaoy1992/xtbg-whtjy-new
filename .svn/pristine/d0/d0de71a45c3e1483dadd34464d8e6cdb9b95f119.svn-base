package com.chinacreator.xtbg.core.common.mail.entity;

import java.io.File;

/**
 *<p>Title:MailAffixBean.java</p>
 *<p>Description:邮件附件</p>
 *<p>Copyright:Copyright (c) 2013</p>
 *<p>Company:湖南科创</p>
