public class MailAffixBean {

	private String affix_id; // 附件id
	private String mail_id; // 邮件id
	private String original_name; // 文件的原始名称
	private String realname; // 系统更改后的文件名称
	private long filesize; // 文件大小
	private File content; // 文件内容
	private String filetype; // 文件类型
	private String filelocation; // 文件所在文件系统的路径地址

	/**
	 * <b>Summary: 获取附件id</b>
	 */
	public String getAffix_id() {
		return affix_id;
	}

	/**
	 * <b>Summary: 设置附件id</b>
	 */
	public void setAffix_id(String affix_id) {
		this.affix_id = affix_id;
	}

	/**
	 * <b>Summary: 获取邮件id</b>
	 */
	public String getMail_id() {
		return mail_id;
	}

	/**
	 * <b>Summary: 设置邮件id</b>
	 */
	public void setMail_id(String mail_id) {
		this.mail_id = mail_id;
	}

	/**
	 * <b>Summary: 获取文件的原始名称</b>
	 */
	public String getOriginal_name() {
		return original_name;
	}

	/**
	 * <b>Summary: 设置文件的原始名称</b>
	 */
	public void setOriginal_name(String original_name) {
		this.original_name = original_name;
	}

	/**
	 * <b>Summary: 获取系统更改后的文件名称</b>
	 */
	public String getRealname() {
		return realname;
	}

	/**
	 * <b>Summary: 设置系统更改后的文件名称</b>
	 */
	public void setRealname(String realname) {
		this.realname = realname;
	}

	/**
	 * <b>Summary: 获取文件大小</b>
	 */
	public long getFilesize() {
		return filesize;
	}

	/**
	 * <b>Summary: 设置文件大小</b>
	 */
	public void setFilesize(long filesize) {
		this.filesize = filesize;
	}

	/**
	 * <b>Summary: 获取文件内容</b>
	 */
	public File getContent() {
		return content;
	}

	/**
	 * <b>Summary: 设置文件内容</b>
	 */
	public void setContent(File content) {
		this.content = content;
	}

	/**
	 * <b>Summary: 获取文件类型</b>
	 */
	public String getFiletype() {
		return filetype;
	}

	/**
	 * <b>Summary: 设置文件类型</b>
	 */
	public void setFiletype(String filetype) {
		this.filetype = filetype;
	}

	/**
	 * <b>Summary: 获取文件所在文件系统的路径地址</b>
	 */
	public String getFilelocation() {
		return filelocation;
	}

	/**
	 * <b>Summary: 设置文件所在文件系统的路径地址</b>
	 */
	public void setFilelocation(String filelocation) {
		this.filelocation = filelocation;
	}

}
