public class FlowWfactionInfoBean {
	
	/**
	 * 流程ID
	 */
	private String flow_id;
	/**
	 * 活动ID
	 */
	private String action_id;
	
	/**
	 * 流程定义ID
	 */
	private String def_id;
	
	/**
	 * 活动名称
	 */
	private String action_name;
	
	/**
	 * 活动别名
	 */
	private String action_alias;
	
	/**
	 * 时限单位
	 */
	private String limit_unit;
	
	/**
	 * 活动时限
	 */
	private String action_limit;
	
	/**
	 * 备注
	 */
	private String remark;
	
	/**
	 * 创建时间
	 */
	private String create_time;

	
	
	public String getFlow_id() {
		return flow_id;
	}

	public void setFlow_id(String flowId) {
		flow_id = flowId;
	}

	public String getAction_id() {
		return action_id;
	}

	public void setAction_id(String actionId) {
		action_id = actionId;
	}

	public String getDef_id() {
		return def_id;
	}

	public void setDef_id(String defId) {
		def_id = defId;
	}

	public String getAction_name() {
		return action_name;
	}

	public void setAction_name(String actionName) {
		action_name = actionName;
	}

	public String getAction_alias() {
		return action_alias;
	}

	public void setAction_alias(String actionAlias) {
		action_alias = actionAlias;
	}

	public String getLimit_unit() {
		return limit_unit;
	}

	public void setLimit_unit(String limitUnit) {
		limit_unit = limitUnit;
	}

	public String getAction_limit() {
		return action_limit;
	}

	public void setAction_limit(String actionLimit) {
		action_limit = actionLimit;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getCreate_time() {
		return create_time;
	}

	public void setCreate_time(String createTime) {
		create_time = createTime;
	}
	
	

	
	
}
