 }
 
/* table的外框的样式 */
#SKbillsheet #fcwebgriddiv  
{
	background-color:#ffffff;BORDER-BOTTOM: 1px solid; BORDER-LEFT: 1px solid; BORDER-RIGHT: 1px solid; BORDER-TOP: 1px solid;BORDER-COLOR:#B8B8B8;
}

/* table的样式 */
#SKbillsheet #fcwebgriddiv #t  
{
	BACKGROUND-COLOR:#FFFFFF;FONT-FAMILY:MS Sans Serif;FONT-SIZE:11px;color:#000000;
	/*background:url(tbodyBG.gif) 0 32px;*/
}
/* 表格的首行 */
#SKbillsheet #fcwebgriddiv #t .fcGridFirstRow 
{
	/*BACKGROUND-COLOR:#c0c0c0 ;FONT-SIZE:12px;FONT-WEIGHT:bold;*/
	BACKGROUND-COLOR:#c0c0c0 ;FONT-SIZE:12px;FONT-WEIGHT:bold;
        background:url('../images/th_img_bg.gif') repeat-x left;
        padding-left:5px;padding-right:5px;
        padding-top:2px;padding-bottom:2px;
        color:#ffffff;
        font-size:14px;font-weight:normal
}
/* 表格的首行td */
#SKbillsheet #fcwebgriddiv #t .fcGridFirstRow td
{
	/*BORDER-RIGHT: #a0a0a0 1px solid;*/
	BORDER-RIGHT: #ffffff 1px solid; 
}

/* table的td样式 */
#SKbillsheet #fcwebgriddiv #t td
{
	border:1px solid #5780C5;padding-left:5px
}

/* 表格控件内的编辑框 */
#SKbillsheet #txtMyGrid 
{
	BACKGROUND-COLOR: white; BORDER-BOTTOM-STYLE: none; BORDER-LEFT-STYLE: none; BORDER-RIGHT-STYLE: none; BORDER-TOP-STYLE: none;
}
/* 垂直滚动条的样式 */
#SKbillsheet #fchtcDiv1
{
	BACKGROUND-COLOR: #EEEDE5; BORDER-RIGHT: gray 0px solid; 
}
/* 水平滚动条的样式 */
#SKbillsheet #fchtcDiv2
{
	BACKGROUND-COLOR: #EEEDE5; BORDER-RIGHT: gray 0px solid; 
}
/*表格右下角的填补块*/
#SKbillsheet #divconer {
	BACKGROUND-COLOR: #EEEDE5; BORDER-BOTTOM: 0px solid; BORDER-RIGHT: 0px solid;font-size:9px;
}

