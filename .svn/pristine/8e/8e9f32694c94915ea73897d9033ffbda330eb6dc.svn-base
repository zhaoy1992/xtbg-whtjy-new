DIV.MainMenuBG    /*主菜单背景（即横向菜单）*/
{
/* border-right: teal 0px solid;
 border-left: #ffffff 0px solid;
 border-bottom: teal 0px solid;*/
 
 font-size: 12px;
 cursor: default;
 color: #ffffff;
 font-family: Verdana, 宋体;
 position: absolute; 
 background-color: none;

 /*BACKGROUND-IMAGE: url("../images/button.gif);*/
}

DIV.MainMenu  /*主菜单某个菜单项（即横向菜单）*/
{
/* border-right: #d6d3ce 1px solid;
 border-top: #d6d3ce 1px solid;
 border-left: #d6d3ce 1px solid;
 border-bottom: #d6d3ce 1px solid;*/
 padding-right: 0px;
 padding-top: 6px;
  padding-left: 8px;
 padding-bottom: 1px;
 cursor: default; 
 position: absolute; 
  /*background-color: #FFFFFF;*/
 /*BACKGROUND-IMAGE: url(common/images/m_r1_c4.jpg);*/
}
DIV.MainMenuOver  /*当鼠标移到主菜单某个菜单项时（即横向菜单）*/
{
/* border-right: #d6d3ce 1px solid;
 border-top: #d6d3ce 1px solid;
 border-left: #d6d3ce 1px solid;
 border-bottom: #08246b 1px solid;*/
 padding-right: 8px;
 padding-top: 6px;
 padding-left: 8px;
 padding-bottom: 1px;
 cursor: default;
 position: absolute;
/* background-color: #FFFFFF;*/
 /*BACKGROUND-IMAGE: url(common/images/m_r1_c4.jpg);*/
}
DIV.MainMenuClick   /*当鼠标单击主菜单某个菜单项时（即横向菜单）*/
{
/* border-right: #636563 1px solid;
 border-top: #636563 1px solid;
 border-left: #636563 1px solid;
 border-bottom: #636563 1px solid;*/
 padding-right: 8px;
 padding-top: 6px;
