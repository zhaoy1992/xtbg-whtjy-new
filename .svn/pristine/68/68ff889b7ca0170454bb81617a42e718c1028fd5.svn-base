<script>
var wftId = creator_getQueryString("wft_id");//事项实例ID;
var ndpId = creator_getQueryString("ndp_id"); //阶段ID;
var piId = creator_getQueryString("pi_id");//事项ID
var read_only = creator_getQueryString("readOnlyFlag");
var user_id = getSysElement('userId'); //用户ID
var bu_id = creator_getQueryString("bu_id"); //执法主体ID
var bu_name = creator_getQueryString("bu_name"); //执法文体名称


/**
 * 初始化函数
 */
 function init() {
     if(ndpId == "10") {
         $('punishTitle').innerHTML = "处罚决定送达";
     } else {
         $('punishTitle').innerHTML = "处罚告知";
     }
     $('djhzList').style.display = "none";
 }

/**
 * 登记回执页面
 */
 function DJHZList() {
     $('djhzList').style.display = "";
     window.frames["DJHZListId"].djhzReload();
     $('wslbList').style.display = "none";
 }
 
/**
 * 文书列表页面
 */
 function WSLBList() {
     $('djhzList').style.display = "none";
     $('wslbList').style.display = "";
 }
 
/**
 *提交处罚告知或处罚决定送达
 */
function submitInformedOrServed() {
    var mattersStatusCode = "PUNISHMENT_NOTICE";
    if(ndpId == "10") {
        mattersStatusCode = "SEND_ARRIVED";
    }
    var tpVo = new publicVO.TaskPlstil();
    tpVo.WFT_ID = wftId; //处罚事项实例ID
    tpVo.NDP_ID = ndpId; //阶段ID
    tpVo.TP_POSTIL = ""; //批注
    tpVo.TP_SYS = ""; //系统批注
    tpVo.TP_STATUS = 1; //状态
    tpVo.TP_EMY = getSysElement("userid"); //批注人
    tpVo.TP_FW_NAME = "";//归属部门
    tpVo.TP_ES_NAME = "";//归属岗位
    tpVo.TP_EMY_SET = "";//批注人组
    tpVo.ISPLATFORM = 0;//是平台结点
    tpVo.SU_ID_INNER = bu_id;//业务单位内码
    tpVo.TP_SYSUNIT_NAME = bu_name;//执法主体
    tpVo.SEGMENT_ID = "";//环节码
    tpVo.TP_INFO = "0";//补充资料
    var rv = PunishInformed.submitInformedOrServed(piId, tpVo, mattersStatusCode);
    if(rv){
        alert("提交处罚告知信息成功！");
    }else{
        alert("提交处罚告知信息失败！");
    }

}
</script>

