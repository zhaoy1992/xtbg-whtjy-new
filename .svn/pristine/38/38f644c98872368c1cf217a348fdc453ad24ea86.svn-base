<?xml version="1.0" encoding="gb2312"?>

<properties>
  <!--内容管理－发布路径-->
  <!--<property name="publishPath" value="d:/publishPath" />-->
  <!--LDAP的相关属性定义-->
  <!--
		获取方法：
		String approot = ConfigManager.getInstance().getConfigValue("approot");
	-->
  <property name="approot" value="D:\workspace\xtbg-whtjy-new/creatorepp"/>
  <!--
		平台功能消息序列：从右往左第一位代表短信，第二位代表内部邮件，第三位代表RTX
	-->
  <property name="messageList" value="sms,rtx,sys"/>
  <!--
		设置iWebOffice当前版本,如果设定的版本低于最新版本，控件会自动更新到最新的版本
	-->
  <property name="iWebOfficeVersion" value="7,9,0,0"/>
  <!-- 
		是否使用消息组件 启用:true 否:false
	 -->
  <property name="usemessage" value="true"/>
  <!-- enableIndex是否允许索引; -->
  <property name="enableIndex" value="true"/>
  <!--
	index.event.protocol 索引事件协议; 
	协议类型:rmi|mina|netty|jgroups|http
-->
  <property name="index.event.protocol" value="netty"/>
  <!--index.event.server索引事件处理服务器地址;取索引服务部署的机器IP-->
  <property name="index.event.server" value="172.16.25.147"/>
  <!--index.event.port 索引事件处理服务器端口; -->
  <property name="index.event.port" value="12347"/>
  <!-- 中讯专用 是否禁用机构用户增删改功能 true or false-->
  <!-- minghua.guo[2010-12-13]-->
  <property name="orguser.modify.forbidden" value="false"/>
  <!-- 标签是否初始化AccessControl -->
  <property name="ENABLE_TAG_SECURITY" value="true"/>
  <!-- 分页列表标签是否显示头部信息（共 条记录） -->
  <property name="enable_page_total_size" value="false"/>
  <!-- 平台类型：
		devegp：开发平台；runegp：运行平台；testegp：测试平台
	-->
  <property name="egptype" value="testegp"/>
  <!--
		平台保留应用ID，以下应用ID将不能被创建
	-->
  <property name="key_appid" value="module"/>
  <!--
		是否允许用户作为一种用户类型的角色，如果为true则将用户帐号作为用户固有的一种用户类型角色
		否则将不做为固定角色，缺省为true
		控制是否允许用户资源授予
		获取方法：
		boolean enableuserrole = ConfigManager.getInstance().getConfigBooleanValue("enableuserrole");
	-->
  <property name="enableuserrole" value="true"/>
  <!-- 系统管理加密方式属性配置 start -->
  <!--
		# Set the following encryption algorithm to encrypt passwords. The default
		# algorithm is SHA (SHA-1). If set to NONE, passwords are stored in the
		# database as plain text. The SHA-512 algorithm is currently unsupported.
		所请求算法的名称
		#passwords.encryption.algorithm=CRYPT - 该加密方式需要加密后的密码，系统管理不支持这种加密方式
		passwords.encryption.algorithm=MD2
		passwords.encryption.algorithm=MD5
		passwords.encryption.algorithm=NONE
		passwords.encryption.algorithm=SHA
		passwords.encryption.algorithm=SHA-256
		passwords.encryption.algorithm=SHA-384
		#passwords.encryption.algorithm=SSHA - 该加密方式需要加密后的密码，系统管理不支持这种加密方式
	-->
  <property name="passwordsEncryptionAlgorithm" value="NONE"/>
  <!--
		定义系统管理密码的加密机制(算法)，目前系统管理中提供以下几种方式：
		MD5：一种不可逆算法，安全
		BASE64：可逆算法，比较安全
		HEX
		NONE：对密码不加密
		和所请求算法的结合进行加密
	-->
  <property name="encrpytype" value="NONE"/>
  <!-- 系统管理加密方式属性配置 end -->
  <!--
		portal加密方式属性配置 start
		一定要与portal的加密配置相同
	-->
  <!--
		# Set the following encryption algorithm to encrypt passwords. The default
		# algorithm is SHA (SHA-1). If set to NONE, passwords are stored in the
		# database as plain text. The SHA-512 algorithm is currently unsupported.
		所请求算法的名称
		#passwords.encryption.algorithm=CRYPT - 该加密方式需要加密后的密码，系统管理不支持这种加密方式
		passwords.encryption.algorithm=MD2
		passwords.encryption.algorithm=MD5
		passwords.encryption.algorithm=NONE
		passwords.encryption.algorithm=SHA
		passwords.encryption.algorithm=SHA-256
		passwords.encryption.algorithm=SHA-384
		#passwords.encryption.algorithm=SSHA - 该加密方式需要加密后的密码，系统管理不支持这种加密方式
	-->
  <property name="portalAlgorithm" value="SHA"/>
  <!--
		定义系统管理密码的加密机制(算法)，目前系统管理中提供以下几种方式：
		MD5：一种不可逆算法，安全
		BASE64：可逆算法，比较安全
		HEX
		NONE：对密码不加密
		和所请求算法的结合进行加密
	-->
  <property name="portalencrpytype" value="BASE64"/>
  <!-- portal加密方式属性配置 end -->
  <!-- 单点登录相关开关 start -->
  <!--
		是否使用cas单点登录功能
		默认为false：不启用
	-->
  <property name="isCasServer" value="false"/>
  <!--
		是否同步portal库中的真实用户，此开关只在电子政务应用中设置为true，
		其他应用可以不配置或则将此开关的值设置为false
		默认为false：不同步
		true：同步
	-->
  <property name="isSynPortalUser" value="true"/>
  <!--
		系统管理版本配置
		默认为1.0，1.0为以前的老版本
		最新系统管理2.0
	-->
  <property name="system.version" value="2.0"/>
  <!-- 单点登录相关开关 end -->
  <!--
		是否允许同一个用户在不同的地方同时登陆系统
		true－允许，缺省值为允许
		false－不允许
	-->
  <property name="enablemutilogin" value="false"/>
  <!--
		用户登录方式
		true-支持用户名登录名与用户实名同时登录方式
		false-仅支持用户登录方式
	-->
  <property name="userloginmode" value="true"/>
  <!--
		是否允许机构角色授予，机构资源授予
		true－允许，缺省值为允许
		false－不允许
	-->
  <property name="enableorgrole" value="true"/>
  
  　<!-- 应用下是否有新增、编辑用户功能 -->
   <property name="orguser.isappnewuser" value="true"/>

  <!--
		是否允许用户组角色授予
		true－允许，缺省值为允许
		false－不允许
	-->
  <property name="enablergrouprole" value="true"/>
  <!--
		岗位是否只显示基本信息
		true：  显示基本信息、岗位隶属机构查询、资源权限查询、岗位授予机构和用户任职情况
		false 只显示基本信息、岗位隶属机构查询
	-->
  <property name="enablejob" value="false"/>
  <!--
		给机构设置机构管理员时，供选择的用户来自:
		current: 只能选择当前机构的用户
		parent：只能选择当前机构及父机构路径的用户
		all：能选择所有机构的用户
	-->
  <property name="enableorgadminall" value="current"/>
  <!--
		是否允许超级管理员之外的用户授予菜单权限:
		true：只允许超级管理员进行菜单权限的授予
		false：所有的人都可以将自己有权限的菜单进行继续授权
	-->
  <property name="enablecolumngrant" value="false"/>
  <!--
		是否允许省级部门管理员特权的开通:
		true:省级部门管理员能管理当前及下级机构的删除、转移和建子机构等所有按钮
		其他级别的部门管理员只有当前及其子机构的修改按钮
		false：所有级别的部门管理员都能管理当前及下级机构的删除、转移和建子机构等所有按钮
	-->
  <property name="enableprovinceadmintequan" value="true"/>
  <!--
		用户查询方式：
		three： 按有主机构用户查询、按无主机构用户查询和按离散用户查询
		two： 按机构用户查询和按离散用户查询
	-->
  <property name="userquerytype" value="two"/>
  <!--
		是否允许用户在机构或者离散用户之间调动
		true：允许
		false：不允许
	-->
  <property name="enableorgusermove" value="false"/>
  <!--
		是否自动生成用户登陆名
		true：自动生成
		false：自定义
	-->
  <property name="isAutoCreateUserName" value="false"/>
  <!--
		退出系统重定向页面窗口，top时退出到根窗口，否则退出到当前窗口
	-->
  <property name="redirecttarget" value="top"/>
  <!-- 是否允许用户有多个机构控制开关 -->
  <property name="sys.user.enablemutiorg" value="false"/>
  <!-- 权限是否关联机构 -->
  <property name="isrolerelatedorg" value="true"/>
  <!-- cs菜单结合控制开关 -->
  <!-- true:打开; false:关闭 -->
  <property name="cs.menu.swtich" value="false"/>
  <!-- 报表菜单结合控制开关 -->
  <!-- true:打开; false:关闭 -->
  <property name="report.menu.swtich" value="false"/>
  <!-- 机构树显示机构ID加机构显示名称 gao.tang 2007.10.29 -->
  <!-- true:打开; false:关闭 -->
  <property name="sys.orgtree.org_no_name_display" value="false"/>
  <!-- 机构管理岗位设置岗位下用户列表调入时，调入所有机构的用户或者调入本机构和离散用户
		本功能已经屏蔽
	-->
  <!-- true:可以调入所有机构下的用户 -->
  <!-- false:只能调入所在机构和子机构下以及离散用户 -->
  <property name="org.job.user.all" value="true"/>
  <!--用户排序方式 -->
  <property name="cms.userOrder.orderMethod" value="same_job_user_sn"/>
  <!--机构排序字段 缺省orgnumber -->
  <!--2007.12.22 ge.tao -->
  <property name="sys.orgOrder.column" value="org_sn"/>
  <!--
		istaxmanager:新增用户显示是否税管员一项
		如果为true,则显示该项
		如果为false，则不显示
	-->
  <property name="istaxmanager" value="false"/>
  <!--
		enableDictButton：屏蔽字典的停用、启用
		deleteDictButton：删除按钮
		如果为true，则显示这些按钮
		如果为false，则将这些按钮灰掉
		取值时默认为true
	-->
  <property name="enableDictButton" value="true"/>
  <property name="deleteDictButton" value="true"/>
  <!--
		机构编号是否按层次规则
		true: 按层次规则，缺省值
		false: 无规则
	-->
  <property name="orgNumberHiberarchy" value="false"/>
  <!-- 集群配置开始  -->
  <!--
		用户会话信息在集群或者多实例应用间复制和同步开关，
		true：表示同步
		false：表示不同步，默认为false
	-->
  <property name="cluster.session.synchronize" value="false"/>
  <!--
		是否使用岗位功能模块
		true：使用
		false：不使用，处理如下
		屏蔽岗位管理菜单
		屏蔽机构的岗位设置
		屏蔽用户的隶属岗位
		屏蔽用户的角色授予的用户对应岗位
		屏蔽用户的基本信息的用户角色权限的查询项的岗位资源
		屏蔽用户的基本信息的用户自身权限的查询项的岗位资源
		屏蔽用户的资源权限的查询项的岗位资源
		屏蔽用户的批量岗位授予
		屏蔽用户的批量资源操作授予的岗位资源
		屏蔽用户的资源授予的岗位资源
		屏蔽角色的资源授予的岗位资源
		屏蔽菜单管理的岗位管理
		屏蔽权限查询的查询对象的机构岗位
		屏蔽权限查询的资源类型的岗位资源
		屏蔽日志查询的记录日志设定的模块列表的岗位管理
		屏蔽个人用户信息修改的资源查看的资源类型的岗位资源
		屏蔽个人用户信息修改的岗位查看
		屏蔽个人用户信息修改的角色查看的用户岗位对应角色
		屏蔽个人用户信息修改的角色查看的用户岗位角色所隶属机构岗位
		************************************************************************************************
		更改开关状态时，需同时更改以下配置文件的设置的状态：
		修改module.xml文件：<item name="岗位管理" id="jobmanage" used="false">
		修改config-manager文件：<resource id="job" name="岗位资源" class="resJobTree.jsp" auto="false" >
		修改config-manager文件：屏蔽所有权限的与岗位相关的操作项
		************************************************************************************************
	-->
  <property name="enablejobfunction" value="false"/>
  <!--
		是否需要资源管理模块
		true－使用
		false－不使用
	-->
  <property name="enablerresmanaer" value="false"/>
  <!--
		Admin是否可以在机构间调动
	-->
  <property name="Administransferred" value="true"/>
  <!--
		控制同一个浏览其中如果一个用户没有退出系统的情况下，是否允许用户再次登录
		缺省允许
	-->
  <property name="session.sameuserenable" value="true"/>
  <!-- 用户调入调出时，设置为fasle保留用户自身权限 -->
  <property name="isdelUserRes" value="false"/>
  <!-- 应用数据库用户创建脚本("createuser.sql") -->
  <property name="sfs_CREATEUSER_FILENAME" value="createuser.sql"/>
  <!-- 创建应用sql执行脚本,按执行顺序添加脚本名称 -->
  <property name="sfs_CREATECOMPONENTS_FILENAME" value="sysmanager.sql,eform.sql,jawe.sql,report.sql,message.sql"/>
  <!-- 应用数据库内容管理创建脚本 -->
  <property name="sfs_CREATECMS_FILENAME" value="createuser.sql"/>
</properties>
