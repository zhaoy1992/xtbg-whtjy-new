	divMain.onmousedown=tab_mouse_down;
	divMain.onmousemove=tab_mouse_move;
	divMain.onmouseup=tab_mouse_up;
	divMain.ondblclick=tab_dblclick;
	SendHttp(location.protocol+"//"+location.host + fcpubdata.servletPath + "/WebBill"+fcpubdata.dotnetVersion+"?GetAllTables","",
		function (result){
			var retX = result.value;
			var obj = result.context;
			obj.outerHTML = SelectAddOption(obj,retX);
		}
	,lstTables);
	
	grid1.DeleteRow();
	grid1.dataset="";


	e_PropWinOnload();
	
}
function uf_ok(){
	var sRet = Trim(txtSql.value);
	if(sRet == "" || divSql.style.display=="none"){
		var s = _PreCheck();
		if(s!=""){
			alert(s);
			return;
		}
		txtSql.value = _GenSql();
	}
	sRet = Trim(txtSql.value);
	if(sRet == ""){
		alert("生成的SQL语句为空!");
		return;
	}
	window.returnValue = sRet;
	CloseBill();
}

function uf_addTable() {
	if(lstTables.selectedIndex<0) return;
	var tableName = lstTables.options(lstTables.selectedIndex).text;
	_addOneTable(tableName);
	
}
//页签控件的点击事件
function PubClickPageControl(index) {
	if(index == 0){
		//alert();
		grid1.ParentPos = "相对" ;
		grid1.hideVscroll = false ;
		grid1.hideHscroll = false ;
		grid1.HScroll();  //重算滚动条
		grid1.VScroll();
		grid1.SetFocus();
	}else	if(index == 1){
		//alert();
		grid2.ParentPos = "相对" ;
		grid2.hideVscroll = false ;
		grid2.hideHscroll = false ;
		grid2.HScroll();  //重算滚动条
		grid2.VScroll();
		grid2.SetFocus();
	}
}
function uf_showsql() {
	var msg1 = "查看SQL语句";
	var msg2 = "返回设计界面";
	if(cmdShow.value ==msg1){
		var s = _PreCheck();
		if(s!=""){
			alert(s);
			return;
		}
		txtSql.value = _GenSql();
		divSql.style.display="";
		divMain.style.display="none";
		lstTables.style.display="none";
		cmdShow.value = msg2;
	}else{
		divSql.style.display='none';
		divMain.style.display='';
		lstTables.style.display='';	
		cmdShow.value = msg1;
	}
}
function uf_runsql() {
	grid3.format="";
	dataset3.format="";
	var num = ToInt(txtRows.value);
	dataset3.PageSize=num;
	dataset3.Open(txtSql.value,"",function (result){});	
}
