/* CSS Document */
body,td,th {
	font-size: 12px;
	margin: 0px;
	padding: 0px;
	
}
/* 链接颜色 */
a:link {color: #435374; text-decoration:none;}
a:visited {color: #435374;text-decoration:none;}
a:hover {color: #435374;POSITION: relative; TOP: 1px;}
a:active {color: #0e4cda;}
/* 表格单颜色*/
.table0a{
	border-collapse:collapse;
	background:#ffffff;
	padding:3px;
	color: #000000;
	margin-top: 5px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
	border: 1px solid #8aa9ca;
	line-height: 22px;
}

.table0a td{
	border:1px solid #B7D3EA;
	color:#435374;
	padding: 3px;
}

.table0a .a{
	color: #486086;
	font-size: 12px;
	background-image: url(../images/bd.jpg);
	background-repeat: repeat-x;
	background-position: top;
}


.buttona {
	font-family: "宋体";
	font-size: 12px;
	color: #435374;
	text-decoration: none;
	background-image: url(../images/tab_02.jpg);
	background-repeat: repeat-x;
	background-position: top;
	text-align: center;
	border: 1px solid #6d96c7;
	padding-top: 2px;
	 cursor: hand
}
