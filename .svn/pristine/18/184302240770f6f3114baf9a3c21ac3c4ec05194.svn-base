<SCRIPT language=javascript>
var selectImg ;

/**
* 展示模式
*/

function showDiv(div,imgsrc,evename){
	alert(div.childNodes[0].outerHTML)
	if(evename != null && evename == "mouseover"){		
	}
}

var showMode = "${showMode}";
function mouseover(img,imgsrc){

  img.src=imgsrc;
  if(selectImg!=img){
      img.className = "hover";
  }
}

function mouseout(img,imgsrc){
  img.src=imgsrc;
  if(selectImg!=img){
      img.className = "normal";
  }
}

function mousedown(img,imgsrc){
  img.src=imgsrc;
  if(selectImg!=img){
      img.className = "down";
  }
}
function mouseup(img,imgsrc){
  img.src=imgsrc;
  if(selectImg!=null){
      selectImg.className = "normal";
  }
  selectImg=img;
  img.className="select";
}

</SCRIPT>

