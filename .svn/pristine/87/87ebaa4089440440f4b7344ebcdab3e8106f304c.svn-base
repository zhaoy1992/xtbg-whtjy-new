public class ExcelDetailBean {
	private int index;		//列序号
	private String code;	//列对应的代码code
	private String name;	//列对应的名称
	
	private String city;	//市
	private String area;	//区
	private String projectName;	//项目名称
	private double amount;	//金额
	private String remark;	//备注
	private String functionItem;	//功能科目
	private String economyItem;		//经济科目
	
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getArea() {
		return area;
	}
	public void setArea(String area) {
		this.area = area;
	}
	public String getProjectName() {
		return projectName;
	}
	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getFunctionItem() {
		return functionItem;
	}
	public void setFunctionItem(String functionItem) {
		this.functionItem = functionItem;
	}
	public String getEconomyItem() {
		return economyItem;
	}
	public void setEconomyItem(String economyItem) {
		this.economyItem = economyItem;
	}
	@Override
	public String toString() {
		return "ExcelDetailBean [index=" + index + ", code=" + code + ", name="
				+ name + ", city=" + city + ", area=" + area + ", projectName="
				+ projectName + ", amount=" + amount + ", remark=" + remark
				+ ", functionItem=" + functionItem + ", economyItem="
				+ economyItem + "]";
	}
}