package com.chinacreator.xtbg.pub.personwork.entity;

public class PersonSearchBean {
	private String userid;
	private String likevalue;//模糊查询值

	private String nameindex;//姓名首字母
	private String orgid;
	private String groupname;//分组查询
	private String listtype;//列表类型
	
	private String department ; //是否是党政一把手
	private String directorylevel; //级别
	private String politics ; //政治面貌
	
	private String pageValue; //是否分页 0:不分页
	private String orgname; //所在单位
	
	public String getGroupname() {
		return groupname;
	}
	public void setGroupname(String groupname) {
		this.groupname = groupname;
	}
	public String getOrgid() {
		return orgid;
	}
	public void setOrgid(String orgid) {
		this.orgid = orgid;
	}
	public String getLikevalue() {
		return likevalue;
	}
	public void setLikevalue(String likevalue) {
		this.likevalue = likevalue;
	}
	public String getUserid() {
		return userid;
	}
	public void setUserid(String userid) {
		this.userid = userid;
	}
	public String getNameindex() {
		return nameindex;
	}
	public void setNameindex(String nameindex) {
		this.nameindex = nameindex;
	}
	public String getListtype() {
		return listtype;
	}
	public void setListtype(String listtype) {
		this.listtype = listtype;
	}
	public String getDepartment() {
		return department;
	}
	public void setDepartment(String department) {
		this.department = department;
	}
	public String getDirectorylevel() {
		return directorylevel;
	}
	public void setDirectorylevel(String directorylevel) {
		this.directorylevel = directorylevel;
	}
	public String getPolitics() {
		return politics;
	}
	public void setPolitics(String politics) {
		this.politics = politics;
	}
	public String getPageValue() {
		return pageValue;
	}
	public void setPageValue(String pageValue) {
		this.pageValue = pageValue;
	}
	public String getOrgname() {
		return orgname;
	}
	public void setOrgname(String orgname) {
		this.orgname = orgname;
	}
	
	
}
