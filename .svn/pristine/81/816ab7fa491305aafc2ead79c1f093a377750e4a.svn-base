package com.chinacreator.xtbg.core.ajax.common.exception;


public class DatabaseException extends ExceptionAbstract{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1898542114L;
	
	public DatabaseException(String errorCode, String errorMsg) {
		super(errorCode, errorMsg,"DATABASE");
	}
	
	public DatabaseException(Exception e){
		super(e);
	}
	public DatabaseException(String errorCode){
		super(errorCode, "","DATABASE");
	}




}
