/************  使用下列样式时在标签属性中加入class="样式名"    *************/
/***********   如class="noborderbutton"    					  *************/

/* 定义带下划线的样式 */
.underlinetext {
	BORDER-TOP: 0px; BORDER-LEFT: 0px; BORDER-RIGHT: 0px; BORDER-BOTTOM: #000000 1px solid;
}

/* 定义四边都带边框样式 */
.aroundlinetext {
	BORDER-TOP: #000000 1px solid; BORDER-LEFT: #000000 1px solid; BORDER-RIGHT: #000000 1px solid; BORDER-BOTTOM: #000000 1px solid;
}

/* 定义没有边框的样式,用在只读的文本框 */
.formtable .nolinetext {
	BORDER: 0px; 
	WIDTH:100%;
	HEIGHT:100%;
}

/* 定义标题样式，在标题所属的td属性里加上class="title" */
.title {
