	padding-top:1px;
}
/*电子监察 审批基本信息表中的环节历史样式  结束*/
/*电子监察 右键菜单  开始*/
UL.dzjc_context_menu{
		background:url("../../images/ef_designer_menu_LeftHalf.gif") repeat-y;
		font-size:12px;
		margin:0px;
		background-color: #F7F3EF;
		padding:1px;
	}
UL.dzjc_context_menu li {
		text-align: left;
		padding:1px;
		margin:0px;
		line-height:22px;
		list-style:none;
		padding-left:22px;
		background-repeat:no-repeat;
		background-position-y: 1px;
		cursor:hand;
	}
UL.dzjc_context_menu li.add{
		background-image: url("../../images/ef_design_new.gif"); 
	}
UL.dzjc_context_menu li.modify{
		background-image: url("../../images/ef_design_shuxing.gif"); 
	}
UL.dzjc_context_menu li.delete{
		background-position-x: 3px;
		background-image: url("../../images/ef_design_delete.gif"); 
	}
UL.dzjc_context_menu li.selected{
		border:1px dotted gray;
		padding:0px;
		padding-left:22px;
		background-color:#FFEFC6; 
	}
/*电子监察 右键菜单  结束*/

