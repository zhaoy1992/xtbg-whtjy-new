/* 定义超链接样式 */
A {
	FONT-SIZE: 12px; COLOR: #0000FF; TEXT-DECORATION: none
}
A:link,A:hover,A:active,A:visited {
	FONT-SIZE: 12px; COLOR: #0000FF
}

/* 定义TABLE样式 */
.formtable {
	BORDER: #415A76 1px solid; 
	border-collapse: collapse;
	table-layout:fix
}
.formtable .tdLabel{
	background-color:#F3F3F3;
}

/* 定义TD默认样式 */
.formtable TD {
	FONT-SIZE: 12px; 
	PADDING-LEFT: 5px;
	BORDER: #415A76 1px solid; 
	HEIGHT:20PX;
}

/* 定义TABLE样式 */
.formtable_red {
	BORDER: red 1px solid; 
	border-collapse: collapse;
	table-layout:fix
}
.formtable1_red .tdLabel{
	background-color:#F3F3F3;
}

/* 定义TD默认样式 */
.formtable_red TD {
	TEXT-ALIGN: left; 
	FONT-SIZE: 12px; 
	PADDING-LEFT: 5px;
	BORDER: red 1px solid; 
	HEIGHT:20PX;
}
/* 定义没有边框的样式,用在只读的文本框 */
.formtable_red .nolinetext {
	BORDER: 0px; 
	WIDTH:100%;
	HEIGHT:100%;
}


/* 定义TABLE样式 */
.formtable_bold {
	BORDER: #000 2px solid; 
	border-collapse: collapse;
	table-layout:fix
}
.formtable1_bold .tdLabel{
	background-color:#F3F3F3;
}

/* 定义TD默认样式 */
.formtable_bold TD {
	TEXT-ALIGN: left; 
	FONT-SIZE: 12px; 
	PADDING-LEFT: 5px;
	BORDER: #000 1px solid; 
	HEIGHT:20PX;
}
/* 定义没有边框的样式,用在只读的文本框 */
.formtable_bold .nolinetext {
	BORDER: 0px; 
	WIDTH:100%;
	HEIGHT:100%;
}

