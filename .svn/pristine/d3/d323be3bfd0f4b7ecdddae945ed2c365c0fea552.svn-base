	width:20px;
	height:20px;
	border:0px;
	}
div.dzjc_tache_list_row button.off_button{
	background:url('../../images/off_button.gif') no-repeat;
	}
div.dzjc_tache_list_row button.on_button{
	background:url('../../images/on_button.gif') no-repeat;
	}
div.dzjc_tache_detail{
	 margin:0px;
	 padding:0px;
	 padding-bottom:1px;
	 display:none;
	 line-height:-1px
	}
