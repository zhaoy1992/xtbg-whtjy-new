package com.chinacreator.xtbg.core.common.mail.entity;

import java.sql.Timestamp;

/**
 *<p>Title:MailRecvBean.java</p>
 *<p>Description:邮件接收人信息</p>
 *<p>Copyright:Copyright (c) 2013</p>
 *<p>Company:湖南科创</p>
 *@author 王博
 *@version 1.0
 *2013-8-6
 */
public class MailRecvBean {

	private String recv_id; // 接收id
	private String mail_id; // 邮件id
	private String recv_user; // 收件人
	private String send_type; // 主送(m)/抄送(c)标志
	private String status; // 状态：1待阅，2已阅，3被撤销，4删除
	private Timestamp read_date; // 阅读时间

	/**
	 * <b>Summary: 获取接收id</b>
	 */
	public String getRecv_id() {
		return recv_id;
	}

	/**
	 * <b>Summary: 设置接收id</b>
	 */
	public void setRecv_id(String recv_id) {
		this.recv_id = recv_id;
	}

	/**
	 * <b>Summary: 获取邮件id</b>
	 */
	public String getMail_id() {
		return mail_id;
	}

	/**
	 * <b>Summary: 设置邮件id</b>
	 */
	public void setMail_id(String mail_id) {
		this.mail_id = mail_id;
	}

	/**
	 * <b>Summary: 获取收件人</b>
	 */
	public String getRecv_user() {
		return recv_user;
	}

	/**
	 * <b>Summary: 设置收件人</b>
	 */
	public void setRecv_user(String recv_user) {
		this.recv_user = recv_user;
	}

	/**
	 * <b>Summary: 获取主送(m)/抄送(c)标志</b>
	 */
	public String getSend_type() {
		return send_type;
	}

	/**
	 * <b>Summary: 设置主送(m)/抄送(c)标志</b>
	 */
	public void setSend_type(String send_type) {
		this.send_type = send_type;
	}

	/**
	 * <b>Summary: 获取状态：1待阅，2已阅，3被撤销，4删除</b>
	 */
	public String getStatus() {
		return status;
	}

	/**
	 * <b>Summary: 设置状态：1待阅，2已阅，3被撤销，4删除</b>
	 */
	public void setStatus(String status) {
		this.status = status;
	}

	/**
	 * <b>Summary: 获取阅读时间</b>
	 */
	public Timestamp getRead_date() {
		return read_date;
	}

	/**
	 * <b>Summary: 设置阅读时间</b>
	 */
	public void setRead_date(Timestamp read_date) {
		this.read_date = read_date;
	}

}
