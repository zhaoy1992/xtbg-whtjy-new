package com.chinacreator.xtbg.core.ajax.common.exception;

public class FileException  extends ExceptionAbstract {

	/**
	 * 
	 */
	private static final long serialVersionUID = 189185142114L;
	
	public FileException(String errorCode, String errorMsg) {
		super(errorCode, errorMsg,"FILE");
	}
	
	public FileException(Exception e){
		super(e);
	}
	public FileException(String errorCode){
		super(errorCode, "","FILE");
	}
}
