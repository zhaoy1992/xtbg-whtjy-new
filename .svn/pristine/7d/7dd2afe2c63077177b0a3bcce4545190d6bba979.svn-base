<script>var arr=new Array();
function uf_open(){
	SetButtonImage(FCButton1,"../image/button/color_button4.jpg");
	SetButtonImage(FCButton2,"../image/button/font.jpg");
}

function uf_ok(){
	var arr=pubDataSet[0] ;
	var obj1=displayfont ;
	var slen=arr.length;
	for(var i=0;i<slen;i++){
		arr[i].style.backgroundColor=obj1.style.backgroundColor;
		arr[i].style.color=obj1.style.color ;
		arr[i].style.fontStyle=obj1.style.fontStyle;
		arr[i].style.textDecoration=obj1.style.textDecoration;
		arr[i].style.fontFamily=obj1.style.fontFamily;
		arr[i].style.fontSize=obj1.style.fontSize;
		arr[i].style.fontWeight=obj1.style.fontWeight;
		switch(SKDBcombobox1.value){
			case '左对齐':{
				arr[i].style.textAlign="left" ;
				break	
			} 	
			case '右对齐':{
				arr[i].style.textAlign="right" ;
				break	
			} 
			case '居中':{
				arr[i].style.textAlign="center" ;
				break	
			} 
		}
		if(SKDBcheckbox1.value=='否'){
			arr[i].nowrap=true;
		}else{
			arr[i].removeAttribute('nowrap');
		}
		arr[i].style.lineHeight=FCDBedit1.value;
		arr[i].style.letterSpacing=FCDBedit2.value;
		switch(SKDBcombobox4.value){
			case '0':{
				//obj.style.filter=uf_filter(0);
				ClearCssPart(arr[i],"filter");
				break;
			}	
			case '90':{
				arr[i].style.filter=uf_filter(1);
				break;
			}	
			case '180':{
				arr[i].style.filter=uf_filter(2);
				break;
			}	
			case '270':{
				arr[i].style.filter=uf_filter(3);
				break;
			}	
		}
	}
	window.close();
}

function uf_filter(i){
	var s="progid:DXImageTransform.Microsoft.BasicImage(Rotation="+i+"); height: 1px";
	return s;	
}
