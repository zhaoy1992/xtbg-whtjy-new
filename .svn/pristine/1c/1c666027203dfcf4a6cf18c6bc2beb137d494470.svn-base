function changeType(sType){
    var sWidth    ="";
	var sPrecision="";
	if(fcpubdata.databaseTypeName=="sqlserver"){
		
		switch(sType){
			case 'int':{
				sWidth = "4";
				break;
			} 
			case 'bigint':{
				sWidth="8";
				break;
			} 
			case 'bit':{
				sWidth="1";
				break;
			} 
			case 'datetime':{
				sWidth="8";
				break;
			} 
			case 'decimal':{
				sWidth="9";
				sPrecision="3";
				break;
			} 
			case 'float':{
				sWidth="8";
				break;
			} 
			case 'image':{
				sWidth="16";
				break;
			} 
			case 'money':{
				sWidth="8";
				break;
			} 
			case 'ntext':{
				sWidth="16";
				break;
			}
			case 'numeric':{
				sWidth="9";
				sPrecision="3"
				break;
			} 
			case 'real':{
				sWidth="4";
				break;
			} 
			case 'smalldatetime':{
				sWidth="4";
				break;
			} 
			case 'smallint':{
				sWidth="2";
				break;
			} 
			case 'smallmoney':{
				sWidth="4";
				break;
			} 
			case 'sql_variant':{
				sWidth="";
				break;
			}
			case 'text':{
				sWidth="16";
				break;
			} 
			case 'timestamp':{
				sWidth="8";
				break;
			}
			case 'tinyint':{
				sWidth="1";
				break;
			} 
			case 'unqueidentifier':{
				sWidth="16";
				break;
			} 
			case 'binary':{
				sWidth="50";
				break;
			}
			case 'varbinary':{
				sWidth="50";
				break;
			} 
			case 'char':{
				sWidth="10";
				break;
			} 
			case 'nchar':{
				sWidth="10";
				break;
			} 
			case 'varchar':{
				sWidth="50";
				break;
			} 
			case 'nvarchar':{
				sWidth="50";
				break;
			} 
		}	
	}
	if(fcpubdata.databaseTypeName=="oracle"){
		switch(sType){
			case 'CHAR':{
				sWidth = "10";
				break;
			}
			case 'VARCHAR2':{
				sWidth = "10";
				break;
			}
			case 'NCHAR':{
				sWidth = "10";
				break;
			}
			case 'NVARCHAR2':{
				sWidth = "10";
				break;
			}
			case 'NUMBER':{
				sWidth = "10";
				sPrecision="0"
				break;
			}
			case 'RAW':{
				sWidth = "10";
				break;
			}
			case 'FLOAT':{
				sWidth = "10";
				break;
			}
		}		
	}
	imgdataset1.bEdit=true;
	imgdataset1.Fields.Field['fdsize'].Value=sWidth;
	imgdataset1.Fields.Field['fddec'].Value=sPrecision;
//	imgdataset1.Fields.Field['isnullable'].Value="NULL";
	imgdataset1.Update("不检查");
	imgdataset1.fset_cont();
	
}

function uf_change(){
	var sType=imgdataset1.Fields.Field['fdtype'].Value;
	changeType(sType);
}

function uf_ok(){
	var TAB=FCDBedit1.value;
	if(IsSpace(Trim(TAB))){
		alert("新建表名不能为空");
		 return;	
	}
	var sSql=uf_strSQL();
	var ZD="";
	var ilen=imgwebgrid1.tab.rows.length;
	for(var i=1;i<ilen;i++){
		var sFd=imgwebgrid1.tab.rows(i).cells(1).innerText;			//字段名
		var sUnique=imgwebgrid1.tab.rows(i).childNodes(7).style.backgroundImage;
		//alert(sUnique); 
		if(sUnique.indexOf("ef_run_grid_checked.gif") >=0 ){
			ZD+=sFd+",";	
		}
	}
	ZD=ZD.substring(0,ZD.length-1);
	var str="";
	if(ZD.length>0){
		if(fcpubdata.databaseTypeName=="sqlserver"){
			str=" CONSTRAINT PK_"+TAB+" PRIMARY KEY CLUSTERED("+ZD+")"	
			
		}
		if(fcpubdata.databaseTypeName=="oracle"){
			str=";alter table "+TAB+" add primary key ("+ZD+")"	
		}
	}
	if(fcpubdata.databaseTypeName=="sqlserver"){	
		var sSql1="create table "+TAB+" ("+sSql+str+")";
		
	}
	if(fcpubdata.databaseTypeName=="oracle"){	
		var sSql1="create table "+TAB+" ("+sSql+")"+str;
	}
	var arr = sSql1.split(";");
	var l= arr.length;
	var sXml = ""
	for(var i=0;i<l;i++){
		var s3=Trim(arr[i])
		if(IsSpace(s3)) continue;
		sXml += "<no>" +s3 + "</no>";
	}
	var sRet=InsertSqls(sXml);
	if(IsSpace(sRet)){
		window.returnValue=TAB;
		alert("成功！");
		CloseBill();
	}else{
		alert(sRet);
	}
}

function uf_strSQL(){
	var sSql="";
	var ilen=imgwebgrid1.tab.rows.length;
	for(var i=1;i<ilen;i++){
		var sFd=imgwebgrid1.tab.rows(i).cells(1).innerText;			//字段名
		var sTxt=imgwebgrid1.tab.rows(i).cells(2).innerText;  		//字段类型
		var WD=imgwebgrid1.tab.rows(i).cells(3).innerText;			//字段长度
		var JD=imgwebgrid1.tab.rows(i).cells(4).innerText;			//字段精度
		var sNull=imgwebgrid1.tab.rows(i).cells(5).innerText;		//允许空
		var sDft=imgwebgrid1.tab.rows(i).cells(6).innerText;  		//默认值
		if(fcpubdata.databaseTypeName=="sqlserver"){
			if(sTxt=="char" || sTxt=="nchar" || sTxt=="nvarchar" || sTxt=="varchar"){
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+"("+WD+") "+sNull+" DEFAULT '"+sDft+"',";
				}else{
					sSql+=sFd+" "+sTxt+"("+WD+") "+sNull+","
				}
			}
			if(sTxt=="decimal" || sTxt=="numeric"){
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+"("+WD+","+JD+") "+sNull+" DEFAULT "+sDft+",";
				}else{
					sSql+=sFd+" "+sTxt+"("+WD+","+JD+") "+sNull+","
				}		
			}
			if(sTxt=="bigint" || sTxt=="binary" || sTxt=="bit" || sTxt=="float" || sTxt=="int" || sTxt=="money" || sTxt=="real" || sTxt=="smallint" || sTxt=="smallmoney" || sTxt=="sql_variant" || sTxt=="tinyint" || sTxt=="varbinary" || sTxt=="datetime" || sTxt=="smalldatetime"){
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" "+sNull+" DEFAULT "+sDft+",";
				}else{
					sSql+=sFd+" "+sTxt+" "+sNull+","
				}
			}
			if(sTxt=="text" || sTxt=="ntext"){
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" "+sNull+" DEFAULT '"+sDft+"',";
				}else{
					sSql+=sFd+" "+sTxt+" "+sNull+","
				}
			}
		}
		var ii=0
		if(fcpubdata.databaseTypeName=="oracle"){
			if(sTxt=="NUMBER"){
				ii++
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" ("+WD+","+JD+") default "+sDft+" "+sNull+","	
				}else{
					sSql+=sFd+" "+sTxt+" ("+WD+","+JD+") "+sNull+","
				}	
			}
			if(sTxt=="FLOAT"){
				ii++
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" ("+WD+") DEFAULT "+sDft+" "+sNull+",";	
				}else{
					sSql+=sFd+" "+sTxt+" ("+WD+") "+sNull+","
				}	
			}
			if(sTxt=="CHAR" || sTxt=="VARCHAR2" || sTxt=="NCHAR" || sTxt=="NVARCHAR2" || sTxt=="RAW"){
				ii++
				if(IsSpace(sDft)==false){
					var ss=sDft;
					if(ss.indexOf("'")!=-1){
						ss=ss.substring(1,ss.length-1)	
					}
					sSql+=sFd+" "+sTxt+" ("+WD+") DEFAULT '"+ss+"' "+sNull+",";
				}else{
					sSql+=sFd+" "+sTxt+" ("+WD+") "+sNull+","
				}	
			}
			if(sTxt=="LONG RAW" || sTxt=="BLOB" || sTxt=="CLOB" || sTxt=="LONG"){
				ii++
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" DEFAULT '"+sDft+" "+sNull+",";	
				}else{
					sSql+=sFd+" "+sTxt+" "+sNull+",";
				}
			}
			if(sTxt=="DATE"){
				ii++
				if(IsSpace(sDft)==false){
					sSql+=sFd+" "+sTxt+" DEFAULT to_date('"+sDft+"','yyyy-mm-dd') "+sNull+","	
				}else{
					sSql+=sFd+" "+sTxt+" "+sNull+",";
				}	
			}
			if(ii=="0"){
				sSql+=sFd+" "+sTxt+" "+sNull+",";
			}
		}
	}
	sSql=sSql.substring(0,sSql.length-1);
	return(sSql);	
