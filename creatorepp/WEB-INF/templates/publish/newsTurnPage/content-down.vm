<!-- 向后翻处理 -->
<script language="javascript">
	if(${currentpage}==${totalpage}){
	    document.write("${nextstr}&nbsp");
	    document.write("${last}");
	}else{
		document.write("<a href='${currentpath}' target='_self'>");
		document.write("${nextstr}&nbsp");
		document.write("</a>");
		document.write("<a href='${lastPageLink}' target='_self'>");
		document.write("${last}");
		document.write("</a>");
	}
	function jumpTo(num){
	    if(num=="") return false;
	    var basePath = "${basePath}";
	    var wholePath = basePath;
	    if(num<=0){
	        num = 1;
	    }else if(num>${totalpage}){
	        num = ${totalpage};
	    }
	    if(num>1){
	        num = num -1 ;		    		    	    
		    if(basePath != ""){
		        var arr = new Array();
		        arr = basePath.split(".");
		        if(arr.length==2){
			        wholePath = arr[0] + "_" + num + "." + arr[1];
		        }
		    }
	    }
	    window.location.href = wholePath;
	}
</script>
<input type="text" name="jumpTo" style="width:40px" value=""> 
<input type="button" value="${jumpToStr}"  onclick="jumpTo(document.all('jumpTo').value)">
</span></div>