		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>classname</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- 内容管理标签定义结束 -->
	
	<!-- 
	导入页面js和css文件：
	/include/pager.js
	/include/jquery-1.4.2.min.js
	/include/treeview.css
	/include/themes/default/easyui.css
	/include/themes/icon.css
	/include/jquery.easyui.min.js
	如果页面上放置config标签后就会引入上述文件
	config标签和csslink，jscript标签都能够自动去重
	 -->
	<tag>
		<name>config</name>
		<tagclass>com.frameworkset.common.tag.pager.config.PageConfig</tagclass>
		<bodycontent>JSP</bodycontent>
		<!-- 
		控制是否生成右键菜单的javascript脚本：enablecontextmenu = true，生成，false-不生成
		 -->
		<attribute>
			<name>enablecontextmenu</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<!-- 通过csslink标签导入的css文件能够自动去重功能 -->
	<tag>
		<name>csslink</name>
		<tagclass>com.frameworkset.common.tag.pager.config.LinkTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<!-- 
		生成link元素：
		<link rel="stylesheet" type="text/css" href="${contextpath}/include/themes/default/easyui.css">
		 -->
		<attribute>
			<name>src</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>
		</attribute>
	</tag>
	<!-- 通过jscript标签导入的js文件能够自动去重功能 -->
	<tag>
		<name>jscript</name>
		<tagclass>com.frameworkset.common.tag.pager.config.ScriptTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<!-- 
		生成script元素：
		<script type="text/javascript" src="${contextpath}/include/jquery-1.4.2.min.js"></script>
		 -->
		<attribute>
			<name>src</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>
		</attribute>
	</tag>
</taglib>
